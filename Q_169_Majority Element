class Solution(object):
    def majorityElement(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        # The majority element is the element that 
        # appears more than ⌊n / 2⌋ times.
        
        # way 1
        #nums.sort()
        #return nums[len(nums)//2]
        
        # way 2
        res=0
        count=0
        
        for i in nums:
            if count==0:
                res=i
            if res==i:
                count+=1
            else:
                count-=1
                
        return res
